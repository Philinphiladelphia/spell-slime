config_version=5

[application]

config/name="Spellslime"
run/main_scene="res://ui/scenes/Test1.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
run/max_fps=30
config/icon="res://icon.svg"
scenes/loading_scene_path="res://addons/yet_another_scene_manager/Scenes/Loading.tscn"
scenes/error_scene_path="res://addons/yet_another_scene_manager/Scenes/Error.tscn"

[autoload]

YASM="*res://addons/yet_another_scene_manager/Scenes.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"

[editor]

movie_writer/movie_file="/home/phil/Desktop/movies/steam_video.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/imjp94.yafsm/plugin.cfg", "res://addons/powder_toy_godot/plugin.cfg", "res://addons/rmsmartshape/plugin.cfg", "res://addons/softbody2d/plugin.cfg", "res://addons/worldmap_builder/plugin.cfg", "res://addons/yet_another_scene_manager/plugin.cfg")

[physics]

